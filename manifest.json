{
    "name": "Browser Events Manager",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "A system to capture, store, and replicate browser events",
    "permissions": [
        "scripting",
        "tabs",
        "history",
        "cookies",
        "downloads",
        "sessions",
        "bookmarks",
        "idle",
        "storage"
    ],
    "background": {
        "service_worker": "index.js"
    },
    "action":{
        "default_title": "Browser Events Manager"
    },
    "host_permissions": [
        "http://127.0.0.1:4999/*"
    ]
}